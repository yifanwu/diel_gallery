{
    "compilerOptions": {
        "outDir": "./dist/",
        // "strict": true,
        "noEmit": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "lib": ["es6", "dom"]
    },
    "include": [
        "src/**/*",
    ],
}
